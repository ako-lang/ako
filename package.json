{
  "name": "ako-lang",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/ako-web.js",
  "bin": "./dist/ako.js",
  "scripts": {
    "build": "run-s build:*",
    "build:cli": "esbuild ./src/flavor/cli.ts --bundle --platform=node --outfile=dist/ako.js",
    "build:node": "esbuild ./src/flavor/node.ts --bundle --platform=node --outfile=dist/node.js",
    "build:web": "esbuild ./src/flavor/web.ts --bundle --target=es2020 --sourcemap --format=esm --outfile=dist/ako-web.js",
    "build:web-light": "esbuild ./src/flavor/web-light.ts --bundle --target=es2020 --sourcemap --format=esm --outfile=dist/ako-weblight.js",
    "build:post": "node ./tools/prepend.js",
    "package": "run-s package:*",
    "package:win": "cd dist && nexe --input=./ako.js --output=./ako.exe --target=win-12.12.0",
    "package:macos": "cd dist && nexe --input=./ako.js --output=./ako-osx --target=darwin-12.12.0",
    "package:linux": "cd dist && nexe --input=./ako.js --output=./ako-linux --target=linux-12.12.0",
    "test": "run-s test:*",
    "test:prebuild": "esbuild ./src/flavor/node.ts --sourcemap=inline --bundle --platform=node --outfile=dist/test.js",
    "test:jest": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^26.0.22",
    "ohm-js": "^15.5.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "esbuild": "^0.11.15",
    "esbuild-jest-transform": "^1.0.4",
    "jest": "^26.6.3",
    "jest-esbuild": "^0.1.6",
    "nexe": "^4.0.0-beta.18",
    "npm-run-all": "^4.1.5"
  }
}
